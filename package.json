{
  "name": "callcenterhelper",
  "version": "1.0.0",
  "description": "",
  "browser": {
    "crypto": "crypto-browserify",
    "stream": "stream-browserify",
    "vm": "vm-browserify"
  },
  "alias": {
    "process": "process/browser"
  },
  "main": "src/main.js",
  "scripts": {
    "start": "webpack serve --mode development --open",
    "test": "jest --runInBand",
    "test:e2e": "npm run build:webpack && node test/e2e/run-e2e.js",
    "dev": "npm run build:dev && concurrently \"npm:sass:watch\" \"npm:start\"",
    "sass": "sass src/styles/main.scss:src/styles/main.css --style compressed",
    "sass:watch": "sass --watch src/styles/main.scss:src/styles/main.css",
    "build:webpack": "npx webpack --mode production",
    "build:dev": "npm run build:webpack && node build.js",
    "build": "npm run clean && npm run build:webpack && node build.js",
    "clean": "node clean-build.js",
    "upload": "npm run build && sh upload.sh"
  },
  "author": "Daniel Hipskind aka Greigh",
  "license": "CC BY-NC-ND",
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.28.5",
    "assert": "^2.1.0",
    "babel-jest": "^30.2.0",
    "babel-loader": "^10.0.0",
    "buffer": "^6.0.3",
    "concurrently": "^9.2.1",
    "crypto-browserify": "^3.12.1",
    "css-loader": "^7.1.2",
    "events": "^3.3.0",
    "html-webpack-plugin": "^5.6.5",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "mini-css-extract-plugin": "^2.9.4",
    "os-browserify": "^0.3.0",
    "playwright": "^1.56.1",
    "process": "^0.11.10",
    "querystring-es3": "^0.2.1",
    "sass": "^1.94.2",
    "sass-loader": "^16.0.6",
    "stream-browserify": "^3.0.0",
    "terser-webpack-plugin": "^5.3.14",
    "url": "^0.11.4",
    "util": "^0.12.5",
    "vm-browserify": "^1.1.2",
    "webpack": "^5.103.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "dependencies": {
    "body-parser": "^2.2.1",
    "chart.js": "^4.5.1",
    "nanoid": "^5.1.6"
  }
}
